@model VideoShop.Models.
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.GenreType.Name</li>
    <li>Release Date: @Model.ReleaseDate.ToString("D")</li>
    <li>Date Added: @Model.Created.ToString("D")</li>
    <li>Number in Stock: @Model.InStock</li>
</ul>

